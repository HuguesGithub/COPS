<div class="card card-outline card-info col">
    <div class="card-body row">
        <div class="col-12 col-md-4">
            <div class="col-12 input-group mb-3">
                <span class="input-group-text col-5">Date de naissance</span>
                <input type="date" class="form-control col-7 ajaxAction" data-objid="%1$s" data-trigger="change" data-ajax="saveData" id="field_birthDate" name="field_birthDate" value="%2$s"/>
            </div>
        </div>
        <div class="col-12 col-md-4">
            <div class="col-12 input-group mb-3">%3$s</div>
            <div class="col-12 input-group mb-3">%5$s</div>
            <div class="col-12 input-group mb-3">%7$s</div>
        </div>
        <div class="col-12 col-md-4">
            <div class="col-12 input-group mb-3">%4$s</div>
            <div class="col-12 input-group mb-3">%6$s</div>
            <div class="col-12 input-group mb-3">%8$s</div>
        </div>
    </div>
</div>
